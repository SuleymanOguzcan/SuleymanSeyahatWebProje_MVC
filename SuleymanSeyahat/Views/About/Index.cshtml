@using SuleymanSeyahat.Models.Siniflar
@model List<Hakkimizda>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
	<div class="wrap">
		<div class="Hakkımızdaaa">
			<h1>Hakkımızda</h1>
			
			@foreach (var x in Model)
			{
				<img src="@x.FotoUrl" alt="" />
				<p>@x.Aciklama</p>
			}
			
		</div>
	</div>
</div>


